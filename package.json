{
  "name": "hm-platform",
  "version": "0.1.0",
  "docsVersion": "beta",
  "description": "Core HM Platform UI",
  "repository": "https://github.com/humanmade/platform-ui",
  "homepage": "https://github.com/humanmade/platform-ui",
  "directories": {
    "bin": "bin"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/index.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start --scripts-version react-wp-scripts",
    "build-js": "NODE_ENV=production react-app-rewired build",
    "postbuild-js": "replace-in-file /\\\"\\/humanmade\\/platform-ui\\//g 'self.registration.scope + \"' build/service-worker.js --isRegex",
    "start": "npm-run-all -p watch-css start-js",
    "build": "NODE_ENV=production npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "author": "Human Made Limited",
  "license": "GPL-3.0",
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewire-svg-react-loader": "github:codebandits/react-app-rewire-svg-react-loader",
    "react-app-rewired": "^1.5.2",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.4.0",
    "react-html-parser": "^2.0.2",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-select": "^1.2.1",
    "react-tabs": "^2.2.2",
    "react-transition-group": "^2.4.0",
    "react-wp-scripts": "github:humanmade/react-wp-scripts#master",
    "reactjs-popup": "^1.1.1",
    "recompose": "^0.26.0",
    "shorthash": "0.0.2",
    "styled-components": "^3.0.0",
    "victory": "^0.24.5",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "dynamic-public-path-webpack-plugin": "^1.0.4",
    "fuzzy": "^0.1.3",
    "husky": "^0.14.3",
    "react-storybook-addon-props-combinations": "^1.0.1",
    "replace-in-file": "^3.4.0"
  }
}
